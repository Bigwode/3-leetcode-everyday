class Solution:
    def romanToInt(self, s):
        """
        :type s: str
        :rtype: int
        """
        MM = {'I': 1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000}
        sum = 0
        for i in range(len(s)):
            if i > 0 and MM[s[i]] > MM[s[i-1]]:
                sum += MM[s[i]] - 2 * MM[s[i-1]]
            else:
                sum += MM[s[i]]
        return sum
